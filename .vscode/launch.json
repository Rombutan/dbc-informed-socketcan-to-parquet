    "version": "0.2.0",
    "configurations": [
        {
            "name": "pls launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/decoder",
            //"args": ["fs.dbc", "--if", "fs3norcal.log", "--file", "--cache", "50", "--of", "/dev/null"],
            "args":["fs.dbc", "--if", "fs-data/FS-3/11222025/11222025_1.log", "--of", "~/Downloads/test.parquet", "--cache", "10", "--file", "--live-decode", "ACC_STATUS_GLV_VOLTAGE"],

            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/../",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}